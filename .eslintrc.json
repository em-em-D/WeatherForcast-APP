{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-shadow": "off",
       "no-param-reassign": "off",
      "eol-last": "off",
      "arrow-parens": "off"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    }
}